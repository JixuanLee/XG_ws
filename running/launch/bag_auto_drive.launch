<launch>

    <!-- <param name="/use_sim_time" value="true" /> -->

    <!-- [0] 加载 车辆模型 -->
    <include file="$(find common)/launch/veh_model.launch"/>

    <!-- [1] 启动 传感器基础静态坐标变换 程序 -->
    <include file="$(find common)/launch/sensors_static_tf.launch"/>

    <!-- [2] 启动 imu_gnss预处理 程序 -->
    <include file="$(find can_unpack)/launch/bag_can_unpack_start.launch"/>

    <!-- [3] 启动 点云拼接&预处理 程序 -->
    <include file="$(find lidar_preprocess)/launch/multi_lidar_preprocess.launch"/>

    <!-- [4] 启动 加载地图 程序 -->
    <include file="$(find ndt_localizer)/launch/map_loader.launch"/>

    <!-- [5] 启动 NDT算法定位 程序 -->
    <include file="$(find ndt_localizer)/launch/ndt_localizer_core.launch"/>

    <!-- [6] 计算全局代价地图和局部代价地图 程序 -->
    <include file="$(find grid_cost_map)/launch/start.launch" />

    <!-- [7] 规划全局路径和局部路径 -->
    <include file="$(find global_planning)/launch/start.launch" />
    <include file="$(find local_planning)/launch/local_planning.launch" />

    <!-- [8] 启动 路径跟随运动控制 程序 -->
    <include file="$(find tracking)/launch/tracking.launch"/>

    <!-- [9] 启动 rviz -->
    <node pkg="rviz" name="rviz" type="rviz" args="-d $(find common)/rviz/control.rviz" />

</launch>
